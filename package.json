{
	"name": "drum-machine",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"react": "^16.7.0",
		"react-dom": "^16.7.0",
		"react-scripts": "2.1.2"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"eject": "react-scripts eject"
	},
	"jest": {
		"setupFiles": [
			"./test/jestsetup.js"
		],
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		],
		"moduleNameMapper": {
			"^.+\\.(css|scss)$": "identity-obj-proxy"
		}
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"babel-core": "^6.18.2",
		"babel-loader": "^6.2.8",
		"babel-preset-es2015": "^6.18.0",
		"babel-preset-react": "^6.16.0",
		"babel-jest": "^23.6.0",
		"enzyme": "^3.8.0",
		"enzyme-adapter-react-16": "^1.7.1",
		"enzyme-to-json": "^3.3.5",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^23.6.0",
		"react-test-renderer": "^16.7.0"
	}
}
